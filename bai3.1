#include<stdio.h>
long nhapmang(int a[], int n){
	for(int i=0;i<n;i++){
		printf("Nhap n[%d]=",i);
		scanf("%d",&a[i]);
	if(a[i]==0)
		return 0;
	}
}
void sapxep(int a[], int n) {
	for (int i = 0;i < n-1;i++) {
		for (int j = i;j < n;j++) {
			if (a[i] > a[j]) {
				int tam = a[i];
				a[i] = a[j];
				a[j] = tam;
			}
		}
	}
}
int main() {
	int a[100];
	int n;
  printf("\nNhap n = ");
	scanf("%d", &n);
	nhapmang(a,n);
	sapxep(a,n);
	printf("\nMang la = ");
	return 0;
}
